	"tokenColors": [
		{
			"comment": ".syntax .c, .syntax .c[ml]",
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#7081BE"
			}
		},
		{
			"name": "Comment.Preproc",
			"scope": "comment.block.preprocessor",
			"settings": {
				"comment": ".syntax .cp",
				"fontStyle": "regular",
				"foreground": "#7081BE"
			}
		},
		{
			"name": "Comment.Special",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"comment": ".syntax .cs",
				"fontStyle": "regular",
				"foreground": "#7081BE"
			}
		},
		{
			"name": "Error",
			"scope": "invalid.illegal",
			"settings": {
				"background": "#722C40",
				"comment": ".syntax .err",
				"foreground": "#E889B5"
			}
		},
		{
			"comment": ".syntax .k, .syntax .k[dpr]",
			"name": "Keyword",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FFFFFF"
			}
		},
		{
			"comment": ".syntax .o, .syntax .ow",
			"name": "Operator",
			"scope": "keyword.operator",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#E4EAF0"
			}
		},
		{
			"name": "Keyword.Constant",
			"scope": [
				"constant.language",
				"support.constant"
			],
			"settings": {
				"comment": ".style .kc",
				//"fontStyle": "bold",
				//"foreground": "#E4EAF0"
				"foreground": "#E3C78A"
			}
		},
		{
			"name": "Keyword.Type",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"comment": ".style .kt",
				"fontStyle": "italic",
				"foreground": "#d3afc5"
			}
		},
		{
			"name": "Name.Attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"comment": ".style .na",
				"foreground": "#90D6CD"
			}
		},
		{
			"name": "Name.Builtin",
			"scope": "variable.other",
			"settings": {
				"comment": ".style .nb",
				"foreground": "#8DC4F0"
			}
		},
		{
			"name": "Name.Builtin.Pseudo",
			"scope": "variable.language",
			"settings": {
				"comment": ".style .bp",
				"foreground": "#B3B2A2"
			}
		},
		{
			"comment": "TODO: support.class is styled as Name.Constant on GitHub.",
			"name": "Name.Class",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"comment": ".style .nc",
				"foreground": "#E3C78A"
			}
		},
		{
			"name": "Name.Constant",
			"scope": "variable.other.constant",
			"settings": {
				"comment": ".style .no",
				"foreground": "#18C9C9"
			}
		},
		{
			"name": "Name.Entity",
			"scope": "constant.character.entity",
			"settings": {
				"comment": ".style .ni",
				"foreground": "#d67c9b"
			}
		},
		{
			"name": "Name.Exception",
			"scope": "entity.name.exception",
			"settings": {
				"comment": ".style .ne",
				"foreground": "#d67c9b"
			}
		},
		{
			"name": "Name.Function",
			"scope": [
				"entity.name.function",
				"support.function",
				"keyword.other.name-of-parameter"
			],
			"settings": {
				"comment": ".style .nf",
				"foreground": "#8be9ee"
			}
		},
		{
			"name": "Name.Namespace",
			"scope": "entity.name.section",
			"settings": {
				"comment": ".style .nn",
				"foreground": "#dabc83"
			}
		},
		{
			"name": "Name.Tag",
			"scope": "entity.name.tag",
			"settings": {
				"comment": ".style .nt",
				"foreground": "#8db3ff"
			}
		},
		{
			"name": "Punctuation.Tag",
			"scope": "punctuation.definition.tag",
			"settings": {
				"comment": ".style .nt",
				"foreground": "#5768b0"
			}
		},
		{
			"name": "Name.Variable",
			"scope": [
				"variable.parameter",
				"support.variable"
			],
			"settings": {
				"comment": ".style .nv, .style .v[cgi]",
				"fontStyle": "italic",
				"foreground": "#18C9C9"
			}
		},
		{
			"name": "Literal.Number",
			"scope": [
				"constant.numeric",
				"constant.other"
			],
			"settings": {
				"comment": ".style .m, .style .m[fhio], .style .il",
				"foreground": "#18C9C9"
			}
		},
		{
			"name": "Literal.String",
			"scope": [
				"string - string source",
				"constant.character"
			],
			"settings": {
				"comment": ".style .s[bcd2ehixl]",
				"foreground": "#D5D5CA"
			}
		},
		{
			"name": "Literal.String.Regex",
			"scope": "string.regexp",
			"settings": {
				"comment": ".style .sr",
				"foreground": "#18C9C9"
			}
		},
		{
			"name": "Literal.String.Symbol",
			"scope": "constant.other.symbol",
			"settings": {
				"comment": ".style .ss",
				"foreground": "#ABD683"
			}
		},
		{
			"name": "Generic.Deleted",
			"scope": "markup.deleted",
			"settings": {
				"background": "#3B2A39",
				"foreground": "#CDBBD6",
				"comment": ".syntax .gd"
			}
		},
		{
			"name": "Generic.Emph",
			"scope": "markup.italic",
			"settings": {
				"comment": ".syntax .ge",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Generic.Error",
			"scope": "markup.error",
			"settings": {
				"comment": ".syntax .gr",
				"foreground": "#DF7788"
			}
		},
		{
			"name": "Generic.Heading",
			"scope": "markup.heading.1",
			"settings": {
				"comment": ".syntax .gh",
				"foreground": "#CC7093"
			}
		},
		{
			"name": "Generic.Inserted",
			"scope": "markup.inserted",
			"settings": {
				"background": "#2C344D",
				"comment": ".syntax .gi"
			}
		},
		{
			"name": "Generic.Output",
			"scope": [
				"markup.output",
